{
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "run_at": "document_start",
      "js": ["index.js"],
      "css": ["katex.css"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "web_accessible_resources": ["index.js", "fonts/*"],
  "permissions": ["*://*.github.com/*", "webNavigation"]
}
